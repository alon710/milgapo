{
    "extends": ["next/core-web-vitals", "eslint:recommended", "next/typescript", "prettier"],
    "plugins": ["unused-imports", "simple-import-sort"],
    "globals": {
        "JSX": "readonly"
    },
    "rules": {
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    }
}
